{
  "name": "@stolostron/search-api",
  "version": "2.4.0",
  "description": "API server for search.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./output/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./output/index.js",
    "start:development": "cross-env NODE_ENV=development node ./output/index.js",
    "build-server": "babel -d ./output ./src -s --ignore **/*.test.js",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build && npm prune --production",
    "clean": "rm -rf output && mkdir output",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stolostron/search-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@graphql-tools/mock": "^8.6.8",
    "@stolostron/security-middleware": "^1.0.4",
    "agentkeepalive": "^4.2.1",
    "ansi-regex": "^6.0.1",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.25.3",
    "async-polling": "^0.2.1",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "log4js": "^6.4.6",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "nconf": "^0.12.0",
    "nocache": "^3.0.3",
    "redis": "^3.1.2",
    "redisgraph.js": "^2.3.0",
    "requestretry": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3"
  }
}
