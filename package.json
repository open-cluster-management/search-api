{
  "name": "search-api",
  "version": "1.0.0",
  "description": "API server for search.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./output/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./output/index.js",
    "start:development": "cross-env NODE_ENV=development node ./output/index.js",
    "build-server": "babel -d ./output ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf output && mkdir output",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext ,.js .",
    "lint:fix": "eslint --ext .jsx,.js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/search-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "agentkeepalive": "^4.0.2",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^1.3.5",
    "async-polling": "^0.2.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.4",
    "express": "^4.17.1",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "graphql-type-json": "^0.2.1",
    "helmet": "^3.12.0",
    "lodash": "^4.17.5",
    "log4js": "^2.5.3",
    "lru-cache": "^4.1.3",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "redis": "^2.8.0",
    "redisgraph.js": "^1.1.4",
    "requestretry": "^1.13.0",
    "security-middleware": "github:open-cluster-management/security-middleware"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.17.0",
    "husky": "^1.1.0",
    "jest": "^23.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^1.17.3",
    "supertest": "^3.1.0"
  }
}
