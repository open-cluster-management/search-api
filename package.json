{
  "name": "@open-cluster-management/search-api",
  "version": "2.2.0",
  "description": "API server for search.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./output/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./output/index.js",
    "start:development": "cross-env NODE_ENV=development node ./output/index.js",
    "build-server": "babel -d ./output ./src -s --ignore **/*.test.js,**/mocks/**",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build && npm prune --production",
    "clean": "rm -rf output && mkdir output",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/search-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@open-cluster-management/security-middleware": "^1.0.2",
    "agentkeepalive": "^4.1.4",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.21.2",
    "async-polling": "^0.2.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "graphql-type-json": "^0.3.2",
    "helmet": "^3.23.3",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "lru-cache": "^5.1.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nocache": "^2.1.0",
    "redis": "^3.0.2",
    "redisgraph.js": "^2.3.0",
    "requestretry": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-tap-reporter": "^1.9.0",
    "nodemon": "^2.0.7",
    "supertest": "^4.0.2"
  }
}
