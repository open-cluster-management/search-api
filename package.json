{
  "name": "search-api",
  "version": "2.0.0",
  "description": "API server for search.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./output/index.js",
    "start:test": "cross-env NODE_ENV=test MOCK=true node ./output/index.js",
    "start:development": "cross-env NODE_ENV=development node ./output/index.js",
    "build-server": "babel -d ./output ./src -s",
    "build": "npm run clean && npm run build-server",
    "build:production": "npm run build",
    "clean": "rm -rf output && mkdir output",
    "shrinkwrap:clean": "del-cli node_modules npm-shrinkwrap.json && npm install --no-optional && npm prune && npm shrinkwrap",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "watch": "nodemon --inspect ./src/index.js --exec babel-node",
    "test": "jest --verbose",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/search-api.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "agentkeepalive": "^4.1.2",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.14.4",
    "async-polling": "^0.2.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "graphql": "^15.1.0",
    "graphql-tools": "^6.0.10",
    "graphql-type-json": "^0.3.2",
    "helmet": "^3.23.1",
    "lodash": "^4.17.15",
    "log4js": "^6.3.0",
    "lru-cache": "^5.1.1",
    "moment": "^2.26.0",
    "morgan": "^1.10.0",
    "nconf": "^0.10.0",
    "nocache": "^2.1.0",
    "redis": "^3.0.2",
    "redisgraph.js": "^2.1.0",
    "requestretry": "^1.13.0",
    "security-middleware": "github:open-cluster-management/security-middleware"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-tap-reporter": "^1.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
